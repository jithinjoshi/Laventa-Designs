            <div class="page-content mt-5">
                <div class="container">
                    <div class="product-details-top mb-2" style="width: 100%;">
                        <div class="row">
                            <div class="col-md-6">
                                <div class="product-gallery product-gallery-vertical">
                                    <div class="row">
                                        <figure class="product-main-image">
                                            <img id="product-zoom" src="/uploads/<%= product.image[0].filename %>"
                                                data-zoom-image="/uploads/<%= product.image[0].filename %>"
                                                alt="product image">

                                            <a href="#" id="btn-product-gallery" class="btn-product-gallery">
                                                <i class="icon-arrows"></i>
                                            </a>
                                        </figure><!-- End .product-main-image -->

                                        <div id="product-zoom-gallery" class="product-image-gallery">
                                            <a class="product-gallery-item active" href="#"
                                                data-image="/uploads/<%= product.image[0].filename %>"
                                                data-zoom-image="/uploads/<%= product.image[0].filename %>">
                                                <img src="/uploads/<%= product.image[0].filename %>" alt="product side">
                                            </a>

                                            <a class="product-gallery-item" href="#"
                                                data-image="/uploads/<%= product.image[1].filename %>"
                                                data-zoom-image="/uploads/<%= product.image[1].filename %>">
                                                <img src="/uploads/<%= product.image[1].filename %>"
                                                    alt="product cross">
                                            </a>

                                            <a class="product-gallery-item" href="#"
                                                data-image="/uploads/<%= product.image[2].filename %>"
                                                data-zoom-image="/uploads/<%= product.image[2].filename %>">
                                                <img src="/uploads/<%= product.image[2].filename %>"
                                                    alt="product with model">
                                            </a>

                                            <a class="product-gallery-item" href="#"
                                                data-image="/uploads/<%= product.image[3].filename %>"
                                                data-zoom-image="/uploads/<%= product.image[3].filename %>">
                                                <img src="/uploads/<%= product.image[3].filename %>" alt="product back">
                                            </a>
                                        </div><!-- End .product-image-gallery -->
                                    </div><!-- End .row -->
                                </div><!-- End .product-gallery -->
                            </div><!-- End .col-md-6 -->

                            <div class="col-md-6">
                                <div class="product-details product-details-centered">
                                    <h1 class="product-title">
                                        <%= product.name %>
                                    </h1><!-- End .product-title -->

                    

                                    <% if(product.discount> 0){ %>
                                        <div class="product-price">
                                            ₹<%= product.offerPrice%>
                                        </div><!-- End .product-price -->

                                        <div>
                                            <span class="ml-4" style="text-decoration:line-through;">₹<%= product.price
                                                    %>
                                            </span>
                                            <span class="ml-4 font-weight-bold text-success">
                                                <%= product.discount %>% off
                                            </span>
                                        </div>


                                        <% }else{ %>
                                            <div class="product-price">
                                                ₹<%= product.price%>
                                            </div><!-- End .product-price -->
                                            <% } %>



                                                <div class="product-content">
                                                    <p>
                                                        <%= product.description %>
                                                    </p>
                                                </div><!-- End .product-content -->


                                                <div class="product-details-action">
                                                    <div class="details-action-col">
                                                        <% if(user){ %>
                                                            <button class="btn-product btn-cart"
                                                                onclick="addToCart('<%= product._id%>')">add to
                                                                cart</button>
                                                            <% }else{ %>
                                                                <a class="btn-product btn-cart" href="/signin">add to
                                                                    cart</a>

                                                                <% } %>

                                                    </div><!-- End .details-action-col -->
                                                </div><!-- End .product-details-action -->

                                                <div class="product-details-footer">
                                                    <div class="product-cat">
                                                        <span class="font-weight-bold">Category:</span>
                                                        <a href="#">
                                                            <%= product.category %>
                                                        </a>

                                                    </div><!-- End .product-cat -->
                                                </div><!-- End .product-details-footer -->
                                </div><!-- End .product-details -->
                            </div><!-- End .col-md-6 -->
                        </div><!-- End .row -->
                    </div><!-- End .product-details-top -->

                    <div class="product-details-tab">

                        <div class="tab-content">
                            <div class="tab-pane fade show active" id="product-desc-tab" role="tabpanel"
                                aria-labelledby="product-desc-link">
                                <div class="product-desc-content">
                                    <h3>Product Description</h3>
                                    <p>
                                        <%= product.description %>
                                    </p>

                                </div><!-- End .product-desc-content -->
                            </div><!-- .End .tab-pane -->
                        </div><!-- End .tab-content -->
                    </div><!-- End .product-details-tab -->

                    <h2 class="title text-center mb-4">You May Also Like</h2><!-- End .title text-center -->

                    <!-- Block2 -->

                    <div class="owl-carousel owl-simple carousel-equal-height carousel-with-shadow" data-toggle="owl" 
                        data-owl-options='{
                            "nav": false, 
                            "dots": true,
                            "margin": 20,
                            "loop": false,
                            "responsive": {
                                "0": {
                                    "items":1
                                },
                                "480": {
                                    "items":2
                                },
                                "768": {
                                    "items":3
                                },
                                "992": {
                                    "items":4
                                },
                                "1200": {
                                    "items":4,
                                    "nav": true,
                                    "dots": false
                                }
                            }
                        }'>
                        <% products.forEach(product => { %>
                            <div class="product product-7 text-center">
                                <figure class="product-media">
                                        <img src="/uploads/<%= product.image[0].filename %> " alt="Product image" class="product-image">
    
                                    <div class="product-action-vertical" onclick="addToWishlist('<%= product._id %>')">
                                        <a class="btn-product-icon btn-wishlist btn-expandable"><span>add to wishlist</span></a>
                                    </div><!-- End .product-action-vertical -->
    
                                    <div class="product-action">
                                        <a href="/productdetails/<%= product._id  %>" class="btn-product"><span>View Product</span></a>
                                    </div><!-- End .product-action -->
                                </figure><!-- End .product-media -->
    
                                <div class="product-body">
                                    <div class="product-cat">
                                        <%= product.category %>
                                    </div><!-- End .product-cat -->
                                    <h3 class="product-title"><%= product.name %> </h3><!-- End .product-title -->
                                    <% if(product.discount > 0){ %>
                                        <div class="product-price">
                                            <span>₹<%= product.offerPrice %></span> <span class="ml-3 text-danger" style="text-decoration:line-through;">₹<%= product.price %></span> <span class="ml-3 text-success"><%= product.discount %> %</span>
                                        </div><!-- End .product-price -->
                                    <% } %> 
                                    
    
                                </div><!-- End .product-body -->
                            </div><!-- End .product -->
                            
                        <% }); %> 
                        
                    </div><!-- End .owl-carousel -->

