        <script>
            function addToCart(prodId) {
                $.ajax({
                    url: '/cart/' + prodId,
                    method: 'get',
                    success: (response) => {
                        if (response.status) {
                            let count = $("#cart-count").html()
                            count = parseInt(count) + 1;
                            $("#cart-count").html(count)
                            location.href = '/cart'

                        }
                    }
                })
            }
        </script>



        <script>
			function addToWishlist(prodId) {
				$.ajax({
					url: '/wishlist/' + prodId,
					method: 'get',
					success: (response) => {
						if (response.status) {
							let count = $("#wishlist-count").html()
							count = parseInt(count) + 1;
							$("#wishlist-count").html(count)
							swal({
								title: "Good job!",
								text: "Item added to the cart",
								timer: 1000,
								icon: "success",
								button: false,
							});
						} else {
							swal({
								title: "oops!",
								text: "Items are out of stock",
								timer: 1000,
								icon: "error",
								button: false,
							});

						}
					}
				})
			}
        </script>

